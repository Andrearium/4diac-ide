   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonFilter
            activeByDefault="true"
            id="org.eclipse.fordiac.ide.fbpaletteviewer.filters.showonlyFBTypesAndSubAppTypes"
            name="Show only IEC 61499 FB Types and Subapp Types"
            visibleInUI="true">
         <filterExpression>
                  <or>
                     <not>
